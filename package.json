{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "utilities",
  "title": "Utilities",
  "description": "My Utility Raycast Extension",
  "icon": "extension-icon.png",
  "author": "koki-develop",
  "owner": "koki-organization",
  "license": "MIT",
  "private": true,
  "commands": [
    {
      "name": "index",
      "title": "Utility Commands",
      "description": "Show all utility commands",
      "mode": "view"
    }
  ],
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "dependencies": {
    "@raycast/api": "1.92.1",
    "@raycast/utils": "1.18.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.11",
    "@types/node": "20.17.19",
    "@types/react": "19.0.10",
    "eslint": "8.57.1",
    "prettier": "3.5.2",
    "typescript": "5.7.3"
  }
}
